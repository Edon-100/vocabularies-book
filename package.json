{
  "name": "vocabularies_killer",
  "version": "1.0.11",
  "productName": "记单词",
  "scripts": {
    "dev:web": "cross-env NODE_FFFF=development webpack -w",
    "build": "npm run build:web && npm run build:ts",
    "build:ts": "npx tsc -p ./utools_ts && node script/copy.js",
    "build:web": "rm -rf dist && cross-env NODE_FFFF=production webpack --mode production",
    "start": "webpack serve --open",
    "test": "rm aaa.js && cross-env NODE_FFFF=production node test.js",
    "dev": "concurrently \"npm run dev:web\" \"npx tsc -w -p ./utools_ts\" \"nodemon --delay 500ms --watch ./utools_ts/assets --exec node script/copy.js\""
  },
  "dependencies": {
