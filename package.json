    "rc-tooltip": "^5.1.1",
    "react": "^17.0.1",
    "react-contenteditable": "^3.3.6",
    "react-dom": "^17.0.1",
    "react-hooks-use-modal": "^2.1.0",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.0.2",
    "use-sound": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.4",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.8",
    "babel-loader": "^8.2.2",
    "babel-plugin-import": "^1.13.3",
    "concurrently": "^6.2.1",
